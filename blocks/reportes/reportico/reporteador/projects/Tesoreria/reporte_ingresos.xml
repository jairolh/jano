DOC.UNIDAD_EJECUTORA UNIDAD,
DTLLE.VIGENCIA_INGRESO VIGENCIA,
DTLLE.SECUENCIAL SECUENCIA,
TO_NUMBER(DOC.NUMERO_EXTERNO) NUMERO_ACTA,
ALE.ESTADO ESTADO,
ALE.FECHA FECHA,
DOC.FECHA_COMPRA_TITULO FECHA_COMPRA_TITULO,
DTLLE.COTE_ID COTE,
DECODE(CONCP.DESCRIPCION,'','SIN DATO',CONCP.DESCRIPCION) DESCRPICION,
DOC.CUBA_NUMERO CUBA_NUMERO ,
TO_CHAR(DTLLE.VALOR, '$999,999,999,999,999.99' )  VALOR_INGRESO
FROM OGT_DETALLE_DOCUMENTO DTLLE
INNER JOIN OGT_DOCUMENTO DOC 
  ON DOC.NUMERO =DTLLE.DOC_NUMERO
  AND DOC.TIPO=DTLLE.DOC_TIPO 
  AND DOC.TIPO='XYZ'
  AND DOC.NUMERO_EXTERNO &lt;&gt; '%'
INNER JOIN (SELECT DOC_E.NUMERO ,DOC_E.NUMERO_EXTERNO,DOC_E.ESTADO,DOC_E.FECHA,DOC_E.TIPO FROM OGT_DOCUMENTO DOC_E ) ALE
  ON ALE.NUMERO=DOC.NUMERO_LEGAL AND ALE.NUMERO_EXTERNO=DOC.NUMERO_EXTERNO AND ALE.TIPO='ALE'
LEFT OUTER JOIN OGT_CONCEPTO_TESORERIA CONCP
  ON CONCP.ID=DTLLE.COTE_ID</SQLRaw></SQL><QueryColumns><QueryColumn><Name>UNIDAD</Name><TableName></TableName><ColumnName>DISTINCT
