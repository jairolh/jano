<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Resumen registro evaluaciones</ReportTitle><ReportDescription>Reporte en el que se indica los jurados asignados a los concursantes y si realizó el registro de la evaluación</ReportDescription><PageSize>US-Legal</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>9</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>show</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>yes</gridSearchable><gridPageable>yes</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT distinct '' column1,
cp.consecutivo_concurso,
ci.consecutivo_inscrito,
p.consecutivo AS consecutivo_persona,
concat( p.tipo_identificacion,' ', p.identificacion) AS documento,
concat( p.nombre, ' ', p.apellido) AS nombre, 
ci.consecutivo_perfil, 
cp.codigo, 
cp.nombre AS perfil,
jrins.id_usuario jurado,
concat(us.nombre, ' ', us.apellido) AS evaluador, 
evaluar.consecutivo_evaluar, 
ceval.nombre AS criterio,
(CASE WHEN ep.puntaje_parcial IS NOT NULL THEN 'SI' ELSE 'NO' END) evaluacion_inicial,
ep.puntaje_parcial puntaje_actual, 
(CASE WHEN recl.id IS NOT NULL THEN 'SI' ELSE 'NO' END) reclamacion,
(CASE WHEN rsta.id IS NOT NULL THEN 'SI' ELSE 'NO' END) respuesta_reclamacion,
(CASE WHEN ep_ant.puntaje_parcial IS NOT NULL THEN 'SI' ELSE 'NO' END) evaluacion_nueva,
ep_ant.puntaje_parcial puntaje_anterior
FROM concurso.concurso_inscrito ci 
INNER JOIN concurso.persona p ON p.consecutivo=ci.consecutivo_persona  
INNER JOIN concurso.concurso_perfil cp ON cp.consecutivo_perfil=ci.consecutivo_perfil
INNER JOIN concurso.concurso conc ON conc.consecutivo_concurso=cp.consecutivo_concurso AND conc.estado='A' AND CURRENT_DATE BETWEEN conc.fecha_inicio::date AND conc.fecha_fin::date
INNER JOIN concurso.concurso_evaluar evaluar ON evaluar.consecutivo_concurso=conc.consecutivo_concurso AND evaluar.estado='A'
INNER JOIN  concurso.criterio_evaluacion ceval ON evaluar.consecutivo_criterio=ceval.consecutivo_criterio
INNER JOIN concurso.jurado_criterio jrcr ON jrcr.id_criterio=evaluar.consecutivo_criterio AND jrcr.estado='A' 
INNER JOIN concurso.jurado_inscrito jrins ON jrins.id_inscrito=ci.consecutivo_inscrito AND jrcr.id_jurado_rol=jrins.id_jurado_rol AND jrins.estado='A'
INNER JOIN jano_usuario us ON UPPER(concat(us.tipo_identificacion, '', us.identificacion))=UPPER(jrins.id_usuario) 
LEFT OUTER JOIN concurso.evaluacion_grupo eg ON eg.id_evaluador=jrins.id_usuario AND eg.id_perfil=cp.consecutivo_perfil AND eg.estado='A' --AND ep.id_grupo=eg.id 
LEFT OUTER JOIN concurso.evaluacion_parcial ep ON ep.id_evaluar = evaluar.consecutivo_evaluar AND ep.id_inscrito=ci.consecutivo_inscrito AND ep.id_grupo=eg.id AND ep.estado='A'  
LEFT OUTER JOIN concurso.evaluacion_parcial ep_ant ON ep_ant.id_evaluar = evaluar.consecutivo_evaluar AND ep_ant.id_inscrito=ci.consecutivo_inscrito AND ep_ant.id_grupo=eg.id AND ep_ant.estado='I'  
LEFT OUTER JOIN concurso.evaluacion_reclamacion recl ON recl.id_inscrito=ci.consecutivo_inscrito AND recl.consecutivo_calendario=evaluar.consecutivo_calendario
LEFT OUTER JOIN  concurso.respuesta_reclamacion rsta ON rsta.id_reclamacion=recl.id 

ORDER BY  ci.consecutivo_inscrito, jrins.id_usuario , ceval.nombre</SQLRaw></SQL><QueryColumns><QueryColumn><Name>consecutivo_inscrito</Name><TableName>ci</TableName><ColumnName>consecutivo_inscrito</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF>4%</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Inscripción</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>codigo</Name><TableName>cp</TableName><ColumnName>codigo</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF>8%</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Código</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>documento</Name><TableName></TableName><ColumnName>concat( p.tipo_identificacion,' ', p.identificacion) AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF>8%</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label