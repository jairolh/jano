<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Resumen validación de requisitos</ReportTitle><ReportDescription>Listado que permite consultar el estado de la verificación de requisitos a los concursantes.</ReportDescription><PageSize>US-Legal</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>show</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>yes</gridSearchable><gridPageable>yes</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT DISTINCT '' id,
ci.consecutivo_inscrito,
conc.consecutivo_concurso AS id_concurso ,
p.consecutivo AS consecutivo_persona,
concat( p.tipo_identificacion,' ', p.identificacion) AS identificacion,
concat( p.nombre, ' ', p.apellido) AS nombre, 
ci.consecutivo_perfil, 
cp.codigo AS codigo,
cp.nombre AS perfil,
(CASE WHEN vr.cumple_requisito IS NOT NULL THEN 'SI' ELSE 'NO' END) validado_requisito
 ,(CASE WHEN recl.id IS NOT NULL THEN 'SI' ELSE 'NO' END) reclamacion
  ,(CASE WHEN rsta.id IS NOT NULL THEN 'SI' ELSE 'NO' END) respuesta_reclamacion
FROM concurso.persona p 
INNER JOIN concurso.concurso_inscrito ci ON p.consecutivo=ci.consecutivo_persona   
INNER JOIN concurso.concurso_perfil cp ON cp.consecutivo_perfil=ci.consecutivo_perfil 
INNER JOIN concurso.concurso conc ON conc.consecutivo_concurso=cp.consecutivo_concurso AND conc.estado='A' AND CURRENT_DATE BETWEEN conc.fecha_inicio::date AND conc.fecha_fin::date
LEFT OUTER JOIN concurso.valida_requisito vr ON ci.consecutivo_inscrito=vr.consecutivo_inscrito AND vr.estado='A' 
LEFT OUTER JOIN concurso.evaluacion_reclamacion recl ON recl.id_inscrito=vr.consecutivo_inscrito 
	AND recl.consecutivo_calendario=(SELECT DISTINCT cal.consecutivo_calendario calendario FROM  concurso.concurso_calendario cal 
					  INNER JOIN concurso.actividad_calendario act ON act.consecutivo_actividad=cal.consecutivo_actividad AND act.nombre IN ('Evaluar requisitos') AND cal.consecutivo_concurso=cp.consecutivo_concurso ) 
LEFT OUTER JOIN  concurso.respuesta_reclamacion rsta ON rsta.id_reclamacion=recl.id 
ORDER BY cp.codigo ,  ci.consecutivo_inscrito


</SQLRaw></SQL><QueryColumns><QueryColumn><Name>identificacion</Name><TableName></TableName><ColumnName>concat( p.tipo_identificacion,' ', p.identificacion) AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF>15%</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>nombre</Name><TableName></TableName><ColumnName>concat( p.nombre, ' ', p.apellido) AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF>20%</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>codigo</Name><TableName></TableName><ColumnName>cp.codigo AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF>10%</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Código</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>perfil</Name><TableName><