<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Consecutivo Disponibilidades Presupuestales</ReportTitle><ReportDescription>Listado de Disponibilidades presupuestales por Rubro</ReportDescription><PageSize>US-Legal</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>7</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>show</gridDisplay><gridSortable>yes</gridSortable><gridSearchable>yes</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT 
RCDP.VIGENCIA VIGENCIA,
RCDP.CODIGO_UNIDAD_EJECUTORA UNIDAD,
RCDP.NUMERO_DISPONIBILIDAD CDP,
CDP.FECHA_REGISTRO FECHA,
LTRIM(RTRIM(REPLACE(CDP.OBJETO,CHR(10),''))) OBJETO,
CDP.NUMERO_OFICIO OFICIO,
CDP.ID_SOLICITANTE ,
(SOL.NOMBRES||' '||SOL.PRIMER_APELLIDO||' '||SOL.SEGUNDO_APELLIDO) NOM_SOLICITANTE, 
RCDP.RUBRO_INTERNO RUBRO,
( RN1.CODIGO||'-'||
 RN2.CODIGO||'-'||
 RN3.CODIGO||'-'||
 RN4.CODIGO||'-'||
 RN5.CODIGO||'-'||
 RN6.CODIGO||'-'||
 RN7.CODIGO||'-'||
 RN8.CODIGO ) COD_RUBRO,
 LTRIM(RTRIM(RUB.DESCRIPCION)) NOM_RUBRO,
RCDP.VALOR VALOR_CDP,
NVL(CDP_ANULADO.VALOR,0) VALOR_ANULADO,
(CASE WHEN CDP_REINT.VALOR_REINT&lt;0 THEN NVL(CDP_REINT.VALOR_REINT*-1,0) ELSE  NVL(CDP_REINT.VALOR_REINT,0) END ) VALOR_REINTEGRO,
EST.RESULTADO ESTADO,
NVL((APR.VALOR+APR.VALOR_MODIFICACIONES),0) TOTAL_APR_RUB,
0 CANT_SEC,
0 SUM_RUBRO,
0 SUM_NULO,
0 SUM_REINT,
0 SUM_TOTAL,
0 SALDO_RUBRO
FROM PR_DISPONIBILIDADES CDP
INNER JOIN PR_DISPONIBILIDAD_RUBRO RCDP
ON CDP.VIGENCIA=RCDP.VIGENCIA AND
CDP.CODIGO_COMPANIA=RCDP.CODIGO_COMPANIA AND
CDP.CODIGO_UNIDAD_EJECUTORA=RCDP.CODIGO_UNIDAD_EJECUTORA AND
CDP.NUMERO_DISPONIBILIDAD=RCDP.NUMERO_DISPONIBILIDAD 
LEFT OUTER JOIN PR_PERSONAS_CDP SOL ON
SOL.SECUENCIAL=CDP.ID_SOLICITANTE
INNER JOIN PR.PR_APROPIACION APR
ON APR.VIGENCIA=RCDP.VIGENCIA
AND APR.CODIGO_COMPANIA = RCDP.CODIGO_COMPANIA
AND APR.RUBRO_INTERNO = RCDP.RUBRO_INTERNO
AND APR.CODIGO_UNIDAD_EJECUTORA = RCDP.CODIGO_UNIDAD_EJECUTORA
INNER JOIN PR_RUBRO RUB
ON RCDP.VIGENCIA = RUB.VIGENCIA AND
RCDP.RUBRO_INTERNO = RUB.INTERNO
INNER JOIN PR_NIVEL1 RN1 ON 
RUB.VIGENCIA = RN1.VIGENCIA AND
RUB.INTERNO_NIVEL1 = RN1.INTERNO AND
RUB.TIPO_PLAN = RN1.TIPO_PLAN
INNER JOIN PR_NIVEL2 RN2 ON 
RUB.VIGENCIA = RN2.VIGENCIA AND
RUB.INTERNO_NIVEL2 = RN2.INTERNO AND
RUB.TIPO_PLAN = RN2.TIPO_PLAN 
INNER JOIN PR_NIVEL3 RN3 ON 
RUB.VIGENCIA = RN3.VIGENCIA AND
RUB.INTERNO_NIVEL3 = RN3.INTERNO AND
RUB.TIPO_PLAN = RN3.TIPO_PLAN 
INNER JOIN PR_NIVEL4 RN4 ON 
RUB.VIGENCIA = RN4.VIGENCIA AND
RUB.INTERNO_NIVEL4 = RN4.INTERNO AND
RUB.TIPO_PLAN = RN4.TIPO_PLAN 
INNER JOIN PR_NIVEL5 RN5 ON 
RUB.VIGENCIA = RN5.VIGENCIA AND
RUB.INTERNO_NIVEL5 = RN5.INTERNO AND
RUB.TIPO_PLAN = RN5.TIPO_PLAN 
INNER JOIN PR_NIVEL6 RN6 ON 
RUB.VIGENCIA = RN6.VIGENCIA AND
RUB.INTERNO_NIVEL6 = RN6.INTERNO AND
RUB.TIPO_PLAN = RN6.TIPO_PLAN 
INNER JOIN PR_NIVEL7 RN7 ON 
RUB.VIGENCIA = RN7.VIGENCIA AND
RUB.INTERNO_NIVEL7 = RN7.INTERNO AND
RUB.TIPO_PLAN = RN7.TIPO_PLAN 
INNER JOIN PR_NIVEL8 RN8 ON 
RUB.VIGENCIA = RN8.VIGENCIA AND
RUB.INTERNO_NIVEL8 = RN8.INTERNO AND
RUB.TIPO_PLAN = RN8.TIPO_PLAN 
INNER JOIN BINTABLAS EST ON 
 EST.GRUPO      = 'PREDIS'
AND EST.NOMBRE       = 'ESTADO_CDP'
AND EST.ARGUMENTO = CDP.ESTADO
LEFT OUTER JOIN 
  (
  SELECT  
  CDP_ANULA.VIGENCIA,
  CDP_ANULA.CODIGO_COMPANIA,
  CDP_ANULA.CODIGO_UNIDAD_EJECUTORA UNIDAD,
  CDP_ANULA.RUBRO_INTERNO RUBRO,
  CDP_ANULA.NUMERO_DISPONIBILIDAD CDP,
  SUM(NVL(CDP_ANULA.VALOR_ANULADO,0)) VALOR
  FROM PR.PR_CDP_ANULADOS CDP_ANULA
  GROUP BY CDP_ANULA.VIGENCIA,
  CDP_ANULA.CODIGO_COMPANIA,
  CDP_ANULA.RUBRO_INTERNO,
  CDP_ANULA.CODIGO_UNIDAD_EJECUTORA,
  CDP_ANULA.NUMERO_DISPONIBILIDAD ) CDP_ANULADO
  ON CDP_ANULADO.VIGENCIA=CDP.VIGENCIA
  AND CDP_ANULADO.CODIGO_COMPANIA=CDP.CODIGO_COMPANIA
  AND CDP_ANULADO.UNIDAD=CDP.CODIGO_UNIDAD_EJECUTORA
  AND CDP_ANULADO.RUBRO=RCDP.RUBRO_INTERNO
  AND CDP_ANULADO.CDP=CDP.NUMERO_DISPONIBILIDAD
LEFT OUTER JOIN 
  ( SELECT TO_NUMBER(DT_ACTA.DOC_VIGENCIA) VIGENCIA,
    SUBSTR(DT_ACTA.DOC_ENTIDAD, 1, 3) CODIGO_COMPANIA,
    SUBSTR(DT_ACTA.DOC_UNIDAD_EJECUTORA, 1, 2) UNIDAD_EJECUTORA,
    DT_ACTA.RUBRO_INTERNO RUBRO,
    DT_ACTA.DISPONIBILIDAD CDP,
    SUM(DT_ACTA.VALOR) VALOR_REINT
    FROM OGT.OGT_ORDEN_PAGO OP
    INNER JOIN  OGT.OGT_DETALLE_ACTAS DT_ACTA
     ON OP.VIGENCIA = DT_ACTA.DOC_VIGENCIA
     AND OP.ENTIDAD = DT_ACTA.DOC_ENTIDAD
     AND OP.UNIDAD_EJECUTORA = DT_ACTA.DOC_UNIDAD_EJECUTORA
     AND OP.TIPO_DOCUMENTO = DT_ACTA.DOC_TIPO_DOCUMENTO
     AND OP.CONSECUTIVO = DT_ACTA.DOC_CONSECUTIVO
    INNER JOIN  OGT.OGT_ACTAS ACTA
     ON DT_ACTA.CONSECUTIVO = ACTA.CONSECUTIVO
     AND DT_ACTA.VIGENCIA = ACTA.VIGENCIA
     AND DT_ACTA.TIPO_DOCUMENTO = ACTA.TIPO_DOCUMENTO
     AND DT_ACTA.ENTIDAD = ACTA.ENTIDAD
     AND DT_ACTA.UNIDAD_EJECUTORA = ACTA.UNIDAD_EJECUTORA
    GROUP BY TO_NUMBER(DT_ACTA.DOC_VIGENCIA) ,
    SUBSTR(DT_ACTA.DOC_ENTIDAD, 1, 3),
    SUBSTR(DT_ACTA.DOC_UNIDAD_EJECUTORA, 1, 2),
    DT_ACTA.RUBRO_INTERNO,
    DT_ACTA.DISPONIBILIDAD) CDP_REINT
  ON CDP_REINT.VIGENCIA=CDP.VIGENCIA
  AND CDP_REINT.CODIGO_COMPANIA=CDP.CODIGO_COMPANIA
  AND CDP_REINT.UNIDAD_EJECUTORA=CDP.CODIGO_UNIDAD_EJECUTORA
  AND CDP_REINT.RUBRO=RCDP.RUBRO_INTERNO
  AND CDP_REINT.CDP=CDP.NUMERO_DISPONIBILIDAD
WHERE
CDP.CODIGO_COMPANIA = 230
ORDER BY
RN1.CODIGO,
 RN2.CODIGO,
 RN3.CODIGO,
 RN4.CODIGO,
 RN5.CODIGO,
RN6.CODIGO,
 RN7.CODIGO,
 RN8.CODIGO,
CDP.FECHA_REGISTRO,CDP.NUMERO_DISPONIBILIDAD
</SQLRaw></SQL><QueryColumns><QueryColumn><Name>VIGENCIA</Name><TableName>RCDP</TableName><ColumnName>VIGENCIA</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF>4%</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>UNIDAD</Name><TableName>RCDP</TableName><ColumnName>CODIGO_UNIDAD_EJECUTORA</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Unidad</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>CDP</Name><TableName>RCDP</TableName><ColumnName>NUMERO_DISPONIBILIDAD</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>FECHA</Name><TableName>CDP</TableName><ColumnName>FECHA_REGISTRO</ColumnNam