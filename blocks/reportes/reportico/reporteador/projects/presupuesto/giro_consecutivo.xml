<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Consecutivos Ordenes de Pago </ReportTitle><ReportDescription>Reporte Presupuestal de  Consecutivos Ordenes de Pago </ReportDescription><PageSize>US-Legal</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>8</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>show</gridDisplay><gridSortable>yes</gridSortable><gridSearchable>yes</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT 
ORDENES.VIGENCIA,
ORDENES.UNIDAD_EJECUTORA,
ORDENES.TIPO_PAGO,
ORDENES.CONSECUTIVO_ORDEN,
ORDENES.FECHA_ORDEN,
ORDENES.VIGENCIA_PRESUPUESTO,
ORDENES.NUMERO_REGISTRO,
ORDENES.NUMERO_DISPONIBILIDAD,
ORDENES.IDENTIFICACION,
ORDENES.BENEFICIARIO,
ORDENES.INT_RUBRO,
ORDENES.COD_RUBRO,
ORDENES.DESCRIPCION_RUBRO,
ORDENES.ESTADO,
ORDENES.VALOR,
ORDENES.ELABORO,
0 CANT_SEC,
0 SUM_RUBRO,
0 SUM_TOTAL
FROM (
SELECT DISTINCT
DOC.ENTIDAD CODIGO_COMPANIA,
DOC.VIGENCIA VIGENCIA,
DOC.UNIDAD_EJECUTORA UNIDAD_EJECUTORA,
DOC.TIPO_DOCUMENTO TIPO_PAGO,
DOC.CONSECUTIVO CONSECUTIVO_ORDEN,
(TO_CHAR(DOC.FECHA_DILIGENCIAMIENTO,'RRRR-MM-DD')) FECHA_ORDEN,
DOC.USUARIO_ELABORO ELABORO,
CRP.VIGENCIA_PRESUPUESTO VIGENCIA_PRESUPUESTO,
CRP.REGISTRO NUMERO_REGISTRO,
CRP.DISPONIBILIDAD NUMERO_DISPONIBILIDAD,
BENE.IB_CODIGO_IDENTIFICACION IDENTIFICACION,
BENE.IB_PRIMER_NOMBRE||' '||BENE.IB_SEGUNDO_NOMBRE||' '||BENE.IB_PRIMER_APELLIDO||' '||BENE.IB_SEGUNDO_APELLIDO BENEFICIARIO,
RUB.INTERNO_RUBRO INT_RUBRO,
RUB.CODIGO_NIVEL1||'.'||RUB.CODIGO_NIVEL2||'.'||RUB.CODIGO_NIVEL3||'.'||RUB.CODIGO_NIVEL4||'.'||RUB.CODIGO_NIVEL5||'.'||RUB.CODIGO_NIVEL6||'.'||RUB.CODIGO_NIVEL7||'.'||RUB.CODIGO_NIVEL8 COD_RUBRO,
RUB.DESCRIPCION DESCRIPCION_RUBRO,
ogt_fn_estado_op(OP.VIGENCIA,OP.ENTIDAD   ,OP.UNIDAD_EJECUTORA   ,OP.CONSECUTIVO) ESTADO,
pk_ogt_op.FN_OGT_BRUTO_DOC (OP.VIGENCIA
                             ,OP.ENTIDAD
                             ,OP.UNIDAD_EJECUTORA
                             ,OP.TIPO_DOCUMENTO
                             ,OP.CONSECUTIVO)  VALOR
FROM OGT.OGT_DOCUMENTO_PAGO DOC
INNER JOIN OGT.OGT_REGISTRO_PRESUPUESTAL CRP 
    ON DOC.VIGENCIA=CRP.VIGENCIA
     AND DOC.ENTIDAD=CRP.ENTIDAD
     AND DOC.UNIDAD_EJECUTORA=CRP.UNIDAD_EJECUTORA
     AND DOC.TIPO_DOCUMENTO=CRP.TIPO_DOCUMENTO
     AND DOC.CONSECUTIVO=CRP.CONSECUTIVO
INNER JOIN OGT.OGT_ORDEN_PAGO OP
    ON DOC.VIGENCIA=OP.VIGENCIA
     AND DOC.ENTIDAD=OP.ENTIDAD
     AND DOC.UNIDAD_EJECUTORA=OP.UNIDAD_EJECUTORA
     AND DOC.TIPO_DOCUMENTO=OP.TIPO_DOCUMENTO
     AND DOC.CONSECUTIVO=OP.CONSECUTIVO
     AND ogt_fn_estado_op(OP.VIGENCIA,OP.ENTIDAD,OP.UNIDAD_EJECUTORA   ,OP.CONSECUTIVO)&lt;&gt;'ANULADA'
INNER JOIN PR.PR_COMPROMISOS COMP 
    ON COMP.VIGENCIA=CRP.VIGENCIA_PRESUPUESTO
     AND COMP.NUMERO_REGISTRO=CRP.REGISTRO
     AND COMP.CODIGO_UNIDAD_EJECUTORA=CRP.UNIDAD_EJECUTORA
INNER JOIN OGT.OGT_INFORMACION_EXOGENA EXOG    
    ON CRP.ENTIDAD=EXOG.ENTIDAD
     AND CRP.UNIDAD_EJECUTORA=EXOG.UNIDAD_EJECUTORA
     AND CRP.VIGENCIA=EXOG.VIGENCIA
     AND CRP.CONSECUTIVO=EXOG.CONSECUTIVO
     AND CRP.RUBRO_INTERNO=EXOG.RUBRO_INTERNO
INNER JOIN PR.PR_V_RUBROS RUB
    ON RUB.VIGENCIA=CRP.VIGENCIA
     AND RUB.INTERNO_RUBRO=CRP.RUBRO_INTERNO
INNER JOIN SHD.SHD_INFORMACION_BASICA BENE
    ON BENE.ID=OP.TER_ID
     AND BENE.IB_FECHA_FINAL IS NULL
     
UNION

SELECT DISTINCT
DOC.ENTIDAD CODIGO_COMPANIA,
DOC.VIGENCIA VIGENCIA,
DOC.UNIDAD_EJECUTORA UNIDAD_EJECUTORA,
DOC.TIPO_DOCUMENTO TIPO_PAGO,
DOC.CONSECUTIVO CONSECUTIVO_ORDEN,
(TO_CHAR(DOC.FECHA_DILIGENCIAMIENTO,'RRRR-MM-DD')) FECHA_ORDEN,
DOC.USUARIO_ELABORO ELABORO,
CRP.VIGENCIA_PRESUPUESTO VIGENCIA_PRESUPUESTO,
CRP.REGISTRO NUMERO_REGISTRO,
CRP.DISPONIBILIDAD NUMERO_DISPONIBILIDAD,
BENE.IB_CODIGO_IDENTIFICACION IDENTIFICACION,
BENE.IB_PRIMER_NOMBRE||' '||BENE.IB_SEGUNDO_NOMBRE||' '||BENE.IB_PRIMER_APELLIDO||' '||BENE.IB_SEGUNDO_APELLIDO BENEFICIARIO,
RUB.INTERNO_RUBRO INT_RUBRO,
RUB.CODIGO_NIVEL1||'.'||RUB.CODIGO_NIVEL2||'.'||RUB.CODIGO_NIVEL3||'.'||RUB.CODIGO_NIVEL4||'.'||RUB.CODIGO_NIVEL5||'.'||RUB.CODIGO_NIVEL6||'.'||RUB.CODIGO_NIVEL7||'.'||RUB.CODIGO_NIVEL8 COD_RUBRO,
RUB.DESCRIPCION DESCRIPCION_RUBRO,
ogt_fn_estado_ra(OP.VIGENCIA,OP.ENTIDAD   ,OP.UNIDAD_EJECUTORA   ,OP.CONSECUTIVO) ESTADO,
pk_ogt_op.FN_VALOR_BRUTORUBRO (OP.VIGENCIA
                             ,OP.ENTIDAD
                             ,OP.UNIDAD_EJECUTORA
                             ,OP.TIPO_DOCUMENTO
                             ,OP.CONSECUTIVO
                             ,CRP.REGISTRO 
                             ,CRP.RUBRO_INTERNO)  VALOR
FROM OGT.OGT_DOCUMENTO_PAGO DOC
INNER JOIN OGT.OGT_REGISTRO_PRESUPUESTAL CRP 
    ON DOC.VIGENCIA=CRP.VIGENCIA
     AND DOC.ENTIDAD=CRP.ENTIDAD
     AND DOC.UNIDAD_EJECUTORA=CRP.UNIDAD_EJECUTORA
     AND DOC.TIPO_DOCUMENTO=CRP.TIPO_DOCUMENTO
     AND DOC.CONSECUTIVO=CRP.CONSECUTIVO
INNER JOIN OGT.OGT_RELACION_AUTORIZACION OP
    ON DOC.VIGENCIA=OP.VIGENCIA
     AND DOC.ENTIDAD=OP.ENTIDAD
     AND DOC.UNIDAD_EJECUTORA=OP.UNIDAD_EJECUTORA
     AND DOC.TIPO_DOCUMENTO=OP.TIPO_DOCUMENTO
     AND DOC.CONSECUTIVO=OP.CONSECUTIVO
     AND ogt_fn_estado_ra(OP.VIGENCIA,OP.ENTIDAD,OP.UNIDAD_EJECUTORA   ,OP.CONSECUTIVO)&lt;&gt;'ANULADA'
INNER JOIN PR.PR_COMPROMISOS COMP 
    ON COMP.VIGENCIA=CRP.VIGENCIA_PRESUPUESTO
     AND COMP.NUMERO_REGISTRO=CRP.REGISTRO
     AND COMP.CODIGO_UNIDAD_EJECUTORA=CRP.UNIDAD_EJECUTORA
INNER JOIN PR.PR_V_RUBROS RUB
    ON RUB.VIGENCIA=CRP.VIGENCIA
     AND RUB.INTERNO_RUBRO=CRP.RUBRO_INTERNO
INNER JOIN SHD.SHD_INFORMACION_BASICA BENE
    ON BENE.IB_CODIGO_IDENTIFICACION='899999230'
     AND BENE.IB_FECHA_FINAL IS NULL) ORDENES
ORDER BY
ORDENES.VIGENCIA,
ORDENES.UNIDAD_EJECUTORA,
ORDENES.TIPO_PAGO,
ORDENES.COD_RUBRO,
ORDENES.DESCRIPCION_RUBRO,
ORDENES.FECHA_ORDEN,
ORDENES.CONSECUTIVO_ORDEN</SQLRaw></SQL><QueryColumns><QueryColumn><Name>VIGENCIA</Name><TableName>ORDENES</TableName><ColumnName>VIGENCIA</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF>4%</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>UNIDAD_EJECUTORA</Name><TableName>ORDENES</TableName><ColumnName>UNIDAD_EJECUTORA</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF>3%</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Unidad</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>TIPO_PAGO</Name><TableName>ORDENES</TableName><ColumnName>TIPO_PAGO</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>center</justify><ColumnWidthPDF>2%</ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>CONSECUTIVO_ORDEN</Name><TableName>ORDEN